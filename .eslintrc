{
    "root": true,
    "parser": "@typescript-eslint/parser",
    "plugins": [
        "@typescript-eslint",
        "prettier"
    ],
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/eslint-recommended",
        "plugin:@typescript-eslint/recommended",
        "prettier"
    ],
    "rules": {
        "no-console": 2, // Means warning
        "prettier/prettier": [
            "error",
            {
                "quoteProps": "consistent",
                "singleQuote": false,
                "tabWidth": 2,
                "trailingComma": "none",
                "useTabs": true
            }
        ]
    }
}